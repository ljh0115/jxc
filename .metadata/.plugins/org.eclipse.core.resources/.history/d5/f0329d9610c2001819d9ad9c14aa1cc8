	public Student2(String stuNo, String name, char sex, String stuClass,
			int age, double score) {
		this.stuNo = stuNo;
		this.name = name;
		this.sex = sex;
		this.stuClass = stuClass;
		this.age = age;
		this.score = score;
	}
